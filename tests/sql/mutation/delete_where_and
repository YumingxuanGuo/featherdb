  id INTEGER PRIMARY KEY DEFAULT 0,
  name STRING DEFAULT NULL INDEX,
  value INTEGER DEFAULT NULL
)
[Integer(2), String("b"), Integer(102)]
[Integer(3), String("c"), Integer(103)]

